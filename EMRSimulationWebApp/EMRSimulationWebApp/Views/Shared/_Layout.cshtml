<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EMRSimulationWebApp</title>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <link href="~/datepicker/jquery-ui.css" rel="stylesheet" />
    <script src="~/datepicker/jquery-ui.js"></script>

    <link href="~/datatables/datatables.min.css" rel="stylesheet" />
    <script src="~/datatables/datatables.min.js"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EMRSimulationWebApp.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/braden.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/foodintake.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/riskman-add.css" />


</head>

<body>
    <header>
        <!-- ADDED id + changed mb-3 -> mb-0 -->
        <nav id="appHeader" class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-0">
            <div class="container-fluid">
                <img src="~/images/unilogo.png" alt="Federation University" style="height:50px">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div>
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <button type="button" id="btnHome" class="btn btn-primary load-patient-list" data-url="@Url.Action("GetPatientList", "Patient")" style="background-color:#000000; width:100px">HOME</button>
                        </li>
                        &nbsp;
                        <li class="nav-item">
                            <button type="button" id="btnLogout" class="btn btn-primary load-logout" style="background-color:#000000; width:100px">LOGOUT</button>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container-fluid">
        <div class="row">
            <!-- Sidebar (ADDED id) -->
            <nav id="appSidebar" class="col-md-2 sidebar bg-dark">
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a href="javascript:void(0);" class="nav-link text-light load-patient-list" data-url="@Url.Action("GetPatientList", "Patient")">Patient List</a>
                    </li>

                    <!-- RISKMAN (global-only; visibility controlled by JS) -->
                    <li id="toggleRiskmanMenu">
                        <button id="toggleRiskman" class="menu-btn">
                            RISKMAN <span class="arrow">▼</span>
                        </button>
                        <ul id="riskmanMenu" class="submenu">
                            <li><a href="javascript:void(0);" class="load-riskman-list" data-url="@Url.Action("GetRiskmanIncidentList", "Patient")">Incident - List</a></li>
                            <li><a href="javascript:void(0);" class="load-riskman-add" data-url="@Url.Action("GetRiskmanIncident", "Patient")">Incident - Add</a></li>
                        </ul>
                    </li>

                    <li id="toggleRecordMenu" style="display: none;">
                        <button id="toggleFlowSheet" class="menu-btn">
                            Flow Sheet <span class="arrow">▼</span>
                        </button>
                        <ul id="flowSheetMenu" class="submenu">

                            <li><a href="javascript:void(0);" class="load-patient-adds" data-url="@Url.Action("GetPatientADDSList", "Patient")">ADDS Chart</a></li>
                            <li><a href="javascript:void(0);" class="load-patient-ivfluid" data-url="@Url.Action("GetPatientIvFluidChartList", "Patient")">IV Fluid Charts</a></li>
                                                        <li><a href="javascript:void(0);" class="load-patient-neurological" data-url="@Url.Action("GetPatientNeurologicalChartList", "Patient")">Neurological Charts</a></li>
                            <li><a href="javascript:void(0);" class="load-patient-fluidbalance" data-url="@Url.Action("GetPatientFluidBalanceChartList", "Patient")">Fluid Balance Charts</a></li>


