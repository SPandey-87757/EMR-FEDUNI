                            <button id="toggleUsecase" class="menu-btn">
                                Patient Use Cases <span class="arrow">▼</span>
                            </button>

                            <ul id="usecaseMenu" class="submenu">
                                <li><a href="javascript:void(0);" class="load-neurological-record" data-url="@Url.Action("GetNeurologicalRecord", "Supervisor")">Add Neurological Record </a></li>
                                <li><a href="javascript:void(0);" class="load-fluid-record" data-url="@Url.Action("GetFluidRecord", "Supervisor")">Add Fluid Balance Record</a></li>
