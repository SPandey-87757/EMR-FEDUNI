    // REPLACE your existing $('.load-braden-add') handler with this version
    $('.load-braden-add').on('click', function (e) {
        e.preventDefault();

        var addUrl    = $(this).data('url');           // /Patient/GetBradenAdd
        var LabId     = $('#txtLabId').val();
        var PatientId = $('#txtPatientId').val();

        // Pre-check: do any Braden rows already exist?
        $.get("/Patient/GetBradenAssessmentList", { labId: LabId, patientId: PatientId })
          .done(function (html) {
              var $tmp = $('<div>').html(html);
              var hasRows = $tmp.find('#bradenTable tbody tr').length > 0;

              if (hasRows) {
                  // Initial exists → show list + info modal
                  $('#content-area').html(html);

                  // If the list’s modal is present, show a clear message
                  var $hdr = $('#bradenInfoHeader');
                  if ($hdr.length) {
                      $hdr.removeClass("bg-success bg-danger text-white text-dark")
                          .addClass("bg-warning text-dark");
                      $('#bradenInfoMessage').text(
                          "An initial Braden assessment already exists. Use ‘Add Assessment’ to record follow-ups."
                      );
                      new bootstrap.Modal(document.getElementById('bradenInfoModal')).show();
                  }
              } else {
                  // No initial → proceed to Add form (original behavior)
                  $.ajax({
                      url: addUrl,
                      method: "GET",
                      data: { labId: LabId, patientId: PatientId },
                      success: function (response) { $('#content-area').html(response); },
                      error: function (xhr, status, error) {
                          console.error("Error loading Braden Add:", error);
                      }
                  });
              }
          })
          .fail(function () {
              // If pre-check fails, fall back to original Add load (don’t block the user)
              $.ajax({
                  url: addUrl,
                  method: "GET",
                  data: { labId: LabId, patientId: PatientId },
                  success: function (response) { $('#content-area').html(response); },
                  error: function (xhr, status, error) {
