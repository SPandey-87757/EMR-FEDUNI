            </nav>

            <!-- Main content (ADDED id) -->
            <main role="main" id="appMain" class="col-md-10 pb-3">
                @RenderBody()
            </main>
        </div>
    </div>
    <!-- Global EMR context (used by all modules) -->
    <input type="hidden" id="txtLabId" />
    <input type="hidden" id="txtPatientId" />

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

